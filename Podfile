# Uncomment this line to define a global platform for your project
platform :ios, '8.0'

use_frameworks!

def import_pods_model
    pod 'EasyMapping'
end

def import_pods
   pod 'AFNetworking'
   pod 'PureLayout'
end



target 'flickrBrowser' do
    import_pods_model
    import_pods
end

target 'flickrBrowserTests' do
    import_pods_model
end
